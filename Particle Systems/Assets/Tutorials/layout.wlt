    Text:
      m_Untranslated: 
    m_Tutorial: {fileID: 0}
    m_Image: {fileID: 2800000, guid: 4828a822ad3fe4312a782e917ab823c0, type: 3}
    m_Video: {fileID: 32900000, guid: 1f962985a7e664c23a58d0fb896ec967, type: 3}
    m_CriteriaCompletion: 0
    m_Criteria:
